    "classnames": "^2.3.2",
    "formik": "^2.4.5",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
